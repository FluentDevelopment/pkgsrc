+/*
+ * NetBSD needs the statvfs structure to be defined without the 
+ * pre-definition of _KERNEL.
+ */
+  
+#include <sys/statvfs.h>
+#endif  /* defined(NETBSDV) && defined(HASSTATVFS) */
+
 #include "lsof.h"
 
 

# $NetBSD: DEINSTALL,v 1.2 2004/05/02 17:50:05 xtraeme Exp $

case "$2" in
	DEINSTALL) rmdir 2>/dev/null @@SERVICEDIR@@ || cat <<EOF

=============================================================
Your service directory @@SERVICEDIR@@ was not removed.
If you no longer need it, remove it by hand.
=============================================================

EOF
	;;
esac

#! /bin/sh
#
# $NetBSD: canna,v 1.4 2002/08/25 19:22:55 jlam Exp $
#
# PROVIDE: canna
# REQUIRE: DAEMON

if [ -f /etc/rc.subr ]
then
	. /etc/rc.subr
fi

name="canna"
rcvar=$name
command="@PREFIX@/sbin/cannaserver"
stop_cmd="@PREFIX@/bin/cannakill"

if [ -f /etc/rc.subr -a -d /etc/rc.d -a -f /etc/rc.d/DAEMON ]
then
	load_rc_config $name
	run_rc_command "$1"
else
	@ECHO@ -n ' ${name}'
	${command} ${canna_flags} ${command_args}
fi

SUBDIRS=	lib add admin create delete info view

.PHONY: all install clean distclean

all install clean:
	@for dir in $(SUBDIRS); do \
		( cd $$dir && $(MAKE) $@ ) || exit 1; \
	done

distclean: clean
	rm -f config.log config.status configure.lineno
	rm -f Makefile */Makefile
	rm -f lib/config.h

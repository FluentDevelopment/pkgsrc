#!/bin/sh
#
# $NetBSD: INSTALL,v 1.1 2001/11/01 01:29:39 zuntum Exp $
#

case $2 in
    PRE-INSTALL)
        ;;
    POST-INSTALL)
    	for i in backup store store/trash download work ; do
	    mkdir -p ${PKG_PREFIX}/Office51/$i
	done
	;;
    *)
	echo "Unexpected Argument $2!!"
	exit 1
	;;
esac

exit 0

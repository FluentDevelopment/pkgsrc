BUILD_DEFS+=		MOZILLA_USE_LINUX

.if defined(MOZILLA_USE_LINUX)
ONLY_FOR_PLATFORM+=	NetBSD-*-i386
.  include "Makefile.Linux.i386"
.else
PKG_SKIP_REASON+=	"No native pre-built NetBSD binary available."

#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1 2001/11/01 00:11:57 zuntum Exp $
#

case $2 in
        DEINSTALL) cat << EOF
===========================================================================
If you don't want to use openldap anymore, remove ${PKG_PREFIX}/etc/openldap.
===========================================================================

EOF
        ;;
esac

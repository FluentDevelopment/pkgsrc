#! /bin/sh
#
# $NetBSD: DEINSTALL,v 1.2 2002/06/20 19:28:24 jlam Exp $

case ${STAGE} in
POST-DEINSTALL)
	for file in au coda-setup-ports codaconfedit; do
		${RM} -f ${PKG_PREFIX}/sbin/${file}
		if [ -f ${PKG_PREFIX}/sbin/vice-${file} ]; then
			${LN} -sf vice-${file} ${PKG_PREFIX}/sbin/${file}
		fi
	done
	;;
esac

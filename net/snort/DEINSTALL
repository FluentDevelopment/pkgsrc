#! /bin/sh
#
# $NetBSD: DEINSTALL,v 1.1 2001/10/31 22:59:01 zuntum Exp $
#

case "$2" in
	DEINSTALL) cat <<EOF

========================================================
If you don't need the logfiles created by snort anymore,
delete the directory /var/log/snort.
========================================================

EOF
	;;
esac

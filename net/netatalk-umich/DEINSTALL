#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.2 2003/08/30 20:22:59 jlam Exp $

PKGNAME=$1
STAGE=$2

case ${STAGE} in
POST-DEINSTALL)
	ETCDIR=/etc/netatalk

	cat << EOF
===========================================================================
If you won't be using ${PKGNAME} any longer, you may want to
remove the netatalk configuration file directory:

	${ETCDIR}
===========================================================================
EOF
	;;
esac
exit 0

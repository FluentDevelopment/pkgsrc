#!/bin/sh
#
# PROVIDE: radiusd
# REQUIRE: network

name="radiusd"
command="!!PREFIX!!/sbin/radiusd"
pidfile="!!PREFIX!!/var/run/radiusd/radiusd.pid"

if [ ! -d /etc/rc.d ]
then
	echo -n ' ${name}'
	exec ${command} ${smbd_flags} ${command_args}
fi

. /etc/rc.subr

load_rc_config $name
run_rc_command "$1"

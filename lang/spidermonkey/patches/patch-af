  * by default since there can be problems with endian-ness and such.
  */
 
+#if !defined(JS_USE_FDLIBM_MATH)
 #if defined(_WIN32) && !defined(__MWERKS__)
 #define JS_USE_FDLIBM_MATH 1
 

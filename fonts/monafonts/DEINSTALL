# $NetBSD: DEINSTALL,v 1.1 2004/11/29 15:00:42 uebayasi Exp $

PREFIX=@PREFIX@
X11BASE=@X11BASE@
X11FONTSPREFIX=${PREFIX}/lib/X11/fonts
X11FONTSSUBDIR=@X11FONTSSUBDIR@
X11FONTSDIR=${X11FONTSPREFIX}/${X11FONTSSUBDIR}

case ${STAGE} in
POST-DEINSTALL)
	${X11BASE}/bin/mkfontdir ${X11FONTSDIR}
	;;
esac
exit 0

# $NetBSD: post-cache,v 1.7 2002/10/30 00:23:00 jlam Exp $

*)
	case $arg in
	*[\`\"\$\\]*)
		arg=`@ECHO@ X$arg | $Xsed -e "$sed_quote_subst"`
		case $arg in
		*[\[\~\#\^\&\*\(\)\{\}\|\;\<\>\?\'\ \	]*|*]*|"")
			arg="\"$arg\""
			;;
		esac
		case $updatecache in
		yes)
			newarg=`@ECHO@ X$arg | $Xsed -e "$sed_quote_subst"`
			@ECHO@ "$arg) arg=\"$newarg\" ;; #1" >> $cache
			cacheupdated=yes
			;;
		esac
		;;
	*[\[\~\#\^\&\*\(\)\{\}\|\;\<\>\?\'\ \	]*|*]*|"")
		arg="\"$arg\""
		case $updatecache in
		yes)
			newarg=`@ECHO@ X$arg | $Xsed -e "$sed_quote_subst"`
			@ECHO@ "$arg) arg=\"$newarg\" ;; #2" >> $cache
			cacheupdated=yes
			;;
		esac
		;;
	-D*)
		arg=`@ECHO@ X$arg | $Xsed -e "$sed_quote_subst"`
		case $arg in
		*[\[\~\#\^\&\*\(\)\{\}\|\;\<\>\?\'\ \	]*|*]*|"")
			arg="\"$arg\""
			;;
		esac
		case $updatecache in
		yes)
			newarg=`@ECHO@ X$arg | $Xsed -e "$sed_quote_subst"`
			@ECHO@ "$arg) arg=\"$newarg\" ;; #3" >> $cache
			cacheupdated=yes
			;;
		esac
		;;
        -[ILRl]*|-Wl,*|/usr/lib/lib*|@LOCALBASE@/*/lib*|@X11BASE@/*/lib*)
		newarg=`@ECHO@ "X$arg" | $Xsed \
			@_BLNK_TRANSFORM_SED@ \
		`
		case $updatecache in
		yes)
			@ECHO@ "$arg) arg=\"$newarg\" ;; #4" >> $cache
			cacheupdated=yes
			;;
		esac
		arg="$newarg"
		;;
	esac
	;;
esac

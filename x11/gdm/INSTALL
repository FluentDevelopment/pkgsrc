# $NetBSD: INSTALL,v 1.2 2003/01/10 17:26:55 jmmv Exp $

if [ "$2" = POST-INSTALL ]
then
	cd @GDMCFGDIR@
	for SOURCE in @GDMCFGFILES@
	do
		TARGET=${SOURCE%.default*}${SOURCE##*.default}
		if [ ! -f $TARGET ]
		then
			@MKDIR@ `dirname $TARGET`
			@CP@ -p $SOURCE $TARGET
		fi
	done
	@LN@ -fs Gnome Sessions/Default
fi

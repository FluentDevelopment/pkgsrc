#!/bin/sh
#
# $NetBSD: INSTALL,v 1.2 2003/08/30 20:23:07 jlam Exp $

PKGNAME=$1
STAGE=$2

case ${STAGE} in
POST-INSTALL)
	cat <<EOF

=============================================================
If you customize namazu configuration, copy
@DOCDIR@/namazu.conf-dist to
@CONFDIR@ and edit them.
=============================================================

EOF
	;;
esac
exit 0

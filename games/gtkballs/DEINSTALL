# $NetBSD: DEINSTALL,v 1.3 2004/03/10 02:18:46 snj Exp $

PKGNAME=$1

case $2 in
POST-DEINSTALL)
	cat << EOF
=============================================================================
If you won't be using ${PKGNAME} any longer, you may want to remove the
score file /var/games/gtkballs-scores.
=============================================================================
EOF
	;;
esac
exit 0

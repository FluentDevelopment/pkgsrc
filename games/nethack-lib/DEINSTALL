#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.2 2003/01/20 09:47:14 wiz Exp $
#

case $2 in
	DEINSTALL) cat << EOF

=============================================================
The records, log and savegames were not wiped out by this
deletion process. If you don't want them around, please
remove ${PKG_PREFIX}/share/nethackdir340.
=============================================================

EOF
	;;
esac

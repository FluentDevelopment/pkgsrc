 	passwd_dialog.rc passwd_dialog.res passwd_dlg.c passwd_dlg.h resource.h
 
-man_MANS = des.1 des_crypt.3
+man_MANS = des_crypt.3
 
 ## this is an awful lot of junk, but it's just as well to include everything
 EXTRA_DIST =		\

#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.3 2004/01/09 19:37:48 jlam Exp $

SASLDB=@PKG_SYSCONFDIR@/sasldb.db

case ${STAGE} in
POST-DEINSTALL)
	if [ -e ${SASLDB} ]
	then
		${CAT} << EOF
===========================================================================
You may want to remove the SASL password data file:

	${SASLDB}
===========================================================================
EOF
	fi
	;;
esac
